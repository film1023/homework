#include <stdio.h>
#include <string.h>
#include <ctype.h> 

int isPalindromeCaseInsensitive(const char *str) {
    int length = strlen(str);
    int i;
    int left = 0;
    int right = length - 1;
    while (left < right) {
        if (tolower(str[left]) != tolower(str[right])) {
            return 0; 
        }
        left++;
        right--;
    }
    return 1; 
}
int main() {
    char word[100]; 
    printf("Enter word: ");
    if (scanf("%s", word) != 1) {
        return 1; 
    }
    if (isPalindromeCaseInsensitive(word)) {
        printf("Pass.\n");
    } else {
        printf("Not Pass.\n");
    }
    return 0;
}
