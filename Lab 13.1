#include <stdio.h>
void swapNumbers(int *ptr1, int *ptr2) {
    int temp;
    temp = *ptr1; 
    *ptr1 = *ptr2; 
    *ptr2 = temp; 
}
int main() {
    int num1, num2;
    printf("Enter num1: ");
    if (scanf("%d", &num1) != 1) return 1;
    printf("Enter num2: ");
    if (scanf("%d", &num2) != 1) return 1;
    int *ptr1 = &num1;
    int *ptr2 = &num2;
    printf("Before swap (num1 & num2): %d, %d\n", num1, num2);
    swapNumbers(ptr1, ptr2); 
    printf("After swap (num1 & num2): %d, %d\n", num1, num2);
    return 0;
}
